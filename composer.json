{
	"name": "diskerror/typed",
	"type": "library",
	"description": "PHP classes to strictly define member structure, to control their data types, and to add convenience methods.",
	"keywords": [
		"type",
		"structure",
		"struct",
		"cast"
	],
	"homepage": "https://github.com/diskerror/typed",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Reid Woodbury",
			"email": "reid@rawsound.com",
			"homepage": "http://www.rawsound.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.1",
		"ext-json": "*",
		"ext-intl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "9.*",
		"phpdocumentor/phpdocumentor": "3.*"
	},
	"suggest": {
		"zendframework/zend-json": "^3.1.0"
	},
	"autoload": {
		"psr-4": {
			"Diskerror\\Typed\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/classes/"
		],
		"psr-4": {
		}
	}
}
