{
	"name": "diskerror/typed",
	"type": "library",
	"description": "PHP classes to strictly define member structure, to control their data types, and to add convenience methods.",
	"keywords": [
		"type",
		"structure",
		"struct",
		"cast"
	],
	"homepage": "https://github.com/diskerror/typed",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Reid Woodbury",
			"email": "reid@rawsound.com",
			"homepage": "http://www.rawsound.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=8.2",
		"ext-json": "*",
		"ext-intl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.5",
		"phpdocumentor/phpdocumentor": "~3.1.2"
	},
	"suggest": {
		"laminas/laminas-json": "~3.1.*",
		"ext-mongodb":"*"
	},
	"autoload": {
		"psr-4": {
			"Diskerror\\Typed\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TestClasses\\": "tests/classes/"
		}
	},
	"config": {
		"allow-plugins": {
			"symfony/flex": true
		}
	}
}
