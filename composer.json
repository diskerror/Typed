{
	"name": "diskerror/typed",
	"type": "library",
	"description": "PHP classes to strictly define member structure, to control their data types, and to add convenience methods.",
	"keywords": [
		"type",
		"structure"
	],
	"homepage": "https://github.com/diskerror/typed",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Reid Woodbury",
			"email": "reidw@rawsound.com",
			"homepage": "http://www.rawsound.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.1",
		"ext-json": "*",
		"ext-intl": "*",
		"mongodb/mongodb": "^1.4"
	},
	"require-dev": {
		"phpunit/phpunit": "7.*",
		"phpdocumentor/phpdocumentor": "2.*",
		"symfony/polyfill-iconv": "^1.8"
	},
	"suggest": {
		"zendframework/zend-json": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"Diskerror\\Typed\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/classes/"
		]
	}
}
